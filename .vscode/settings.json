{
  "editor.tabSize": 2,
  "editor.indentSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "tailwindCSS.experimental.classRegex": [
    ["([\"'`][^\"'`]*.*?[\"'`])", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    ".*ClassName"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
